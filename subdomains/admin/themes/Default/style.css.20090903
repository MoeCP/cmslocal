TABLE.homeTable {
  background-color: #ffffff;
  margin: 0px;
  padding: 4px;
  border: 1px solid #8f8377;
  border-spacing: 0px;
  border-collapse: collapse;
}

TABLE.homeTable TH {
  border-bottom: 1px solid #8f8377;
  border-right: 1px solid #8f8377;
}

TABLE.homeTable TD {
  border-bottom: 1px solid #8f8377;
  border-right: 1px solid #8f8377;
  color: #000000;
}

table.homeTable td a:link, table.homeTable td a:visited {
	color: #000000;
	text-decoration: underline;
}

table.homeTable td a:hover {
	text-decoration: none;
}



table.sortableTable {
	background-color: #ffffff;
    margin: 0px;
    padding: 4px;
    border: 1px solid #8f8377;
    border-spacing: 0px;
    border-collapse: collapse;
}

table.sortableTable TR.even TD {
  color: #000000;
}
table.sortableTable TR.odd TD {
  color: #000000;
}

table.sortableTable TR.rejected TD {
  color: #000000;
}

table.sortableTable td a:link, table.sortableTable td a:visited {
	color: #000000;
	text-decoration: underline;
}

table.sortableTable td a:hover {
	text-decoration: none;
}

table.sortableTable tr.even {
	background-color: #F9F9F9;
}
table.sortableTable tr.odd {
	background-color: #FFF;
}

table.sortableTable tr.rejected {
  background-color: #DA7166;
}

table.sortableTable td {
	border-bottom: 1px solid #e5e5e5;
	padding: 4px;
}
table.sortableTable tr.even:hover {
	background-color: #f1f1f1;
}
table.sortableTable tr.odd:hover {
	background-color: #f1f1f1;
}
table.sortableTable tr.rejected:hover {
	background-color: #f1f1f1;
}
table.sortableTable td.options {
	background-color: #ffffff;
	font-size: 8px;
}

