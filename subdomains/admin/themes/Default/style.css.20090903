/********************************************************
NAVIGATION STYLES
********************************************************/

#menu {
	height:32px;
	position:relative;
	margin-top: 5px;
	margin-bottom: 10px;
	z-index:1000; 
	float:left; 
	width: 100%;
	margin-left: 0;
	margin-right: 0;
	background: url(/image/newstyle/class.png) repeat left top;
	border: 1px solid #a6a6a6;
	font-family: Arial, Tahoma;
}

#menu table.ie6 {border-collapse:collapse; margin:0; padding:0;}

#menu ul {padding:0; margin:0; list-style: none;}
#menu ul li {float:left;}
#menu ul li a {
	display:block; 
	float:left;
	padding:0 15px 0 0;
	text-decoration:none; 
	color:#404040;
	text-shadow: 0 1px 1px #fff;  
	font-weight:bold; 
	height:32px;
	line-height:34px;
	background:url(multicolumn/tabCenter.gif) no-repeat right top;
}

#menu ul li a b {
	display:block; 
	float:left;
	padding:0 0 0 7px; 
	text-decoration:none; 
	font-size: 14px;
	color:#404040;
	text-shadow: 0 1px 1px #fff; 
	font-weight:bold; 
	height: 32px;
	line-height:34px; 
	background:url(multicolumn/tabCenter.gif) no-repeat left top; 
	cursor:pointer;
}

.home, .five {
	background: url(/image/newstyle/home.png) left no-repeat;
	padding-left: 23px;
}

.two {
	background: url(/image/newstyle/two.png) left no-repeat;
	padding-left: 23px;	
}

.three {
	background: url(/image/newstyle/campaign.png) left no-repeat;
	padding-left: 23px;	
}

.four {
	background: url(/image/newstyle/reporting.png) left no-repeat;
	padding-left: 23px;	
}

.five {
	background: url(/image/newstyle/settings.png) left no-repeat;
	padding-left: 23px;		
}

.six {
	background: url(/image/newstyle/users.png) left no-repeat;
	padding-left: 23px;		
}

#menu ul li div.holder {position:absolute; left:-9999px;}

#menu ul li div .leftSide {float:left;background: #ddd url(multicolumn/dropLeft.gif) no-repeat left bottom;}

#menu ul li div .rightSide {
	float:left; 
	display: inline; 
	padding: 0; 
}

#menu ul li:hover {position:relative;}
#menu ul li a:hover {color:#910; background-position:right center; white-space:nowrap; position:relative;}
#menu ul li a:hover b {color:#910; background-position:left center;}
#menu ul li a.sub:hover {color:#910; background-position:right bottom; white-space:nowrap; position:relative;}
#menu ul li a.sub:hover b {color:#910; background-position:left bottom;}

#menu ul li:hover > a {color:#910; position:relative;}
#menu ul li:hover a.sub {color:#910; background-position:right bottom; white-space:nowrap; position:relative;}
#menu ul li:hover a.sub > b {color:#910; background-position:left bottom;}

#menu ul :hover div.holder {position:absolute; top:33px; left:4px; margin: 0; padding: 0;}

#menu ul li.right a:hover div.holder {left:auto; right:3px; top:33px;}
#menu ul li.right:hover div.holder {left:auto; right:4px; top:33px;}

#menu ul dl {
	width: 195px; 
	margin: 0 0 10px 0; 
	padding: 5px;
	list-style:none;
}

#menu ul dl dt {
	padding: 3px; 
	margin: 0; 
	line-height: 17px; 
	font-size: 13px;
	font-weight: bolder;
	letter-spacing: 1.6px;
	background: #1A4EA1; 
	color: #fff;
	text-shadow: 0 1px 1px #404040; 
	white-space: nowrap;
}

#menu ul dl dd {
	display:block; 
	padding:0; 
	margin:0;
}

#menu ul dd a {
	display:block; 
	height: 17px; 
	line-height: 15px; 
	font-size: 13px;
	color:#404040; 
	text-align:left; 
	margin: 0; 
	padding: 2px;
	font-weight: bold; 
	white-space:nowrap; 
	float:none;
}

#menu ul dd a:hover {color:#c60;}

#menu ul table td {vertical-align:top;}


/********************************************************
END NAVIGATION STYLES
********************************************************/
